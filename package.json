{
  "name": "pwas",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "bar-pwa": "cd bar-pwa && npm run serve -- --port 8080",
    "bubble-pwa": "cd bubble-pwa && npm run serve -- --port 8081",
    "graph-pwa": "cd graph-pwa && npm run serve -- --port 8082",
    "grid-pwa": "cd grid-pwa && npm run serve -- --port 8083",
    "heat-pwa": "cd heat-pwa && npm run serve -- --port 8084",
    "pie-pwa": "cd pie-pwa && npm run serve -- --port 8085",
    "server": "cd server && nodemon app.js",
    "serve-all": "npm-run-all --parallel --aggregate-output bubble-pwa grid-pwa bar-pwa graph-pwa heat-pwa pie-pwa",
    "build-bar-pwa": "cd bar-pwa && npm run build",
    "build-bubble-pwa": "cd bubble-pwa && npm run build",
    "build-graph-pwa": "cd graph-pwa && npm run build",
    "build-grid-pwa": "cd grid-pwa && npm run build",
    "build-heat-pwa": "cd heat-pwa && npm run build",
    "build-pie-pwa": "cd pie-pwa && npm run build",
    "build-all": "npm-run-all --parallel --aggregate-output build-bar-pwa build-bubble-pwa build-graph-pwa build-grid-pwa build-heat-pwa build-pie-pwa",
    "bar-pwa-built": "cd bar-pwa && http-server dist/ p5000",
    "bubble-pwa-built": "cd bubble-pwa && http-server dist/ p5001",
    "graph-pwa-built": "cd graph-pwa && http-server dist/ p5002",
    "grid-pwa-built": "cd grid-pwa && http-server dist/ p5003",
    "heat-pwa-built": "cd heat-pwa && http-server dist/ p5004",
    "pie-pwa-built": "cd pie-pwa && http-server dist/ p5005",
    "run-all-built": "npm-run-all --parallel --aggregate-output bar-pwa-prod bubble-pwa-prod graph-pwa-prod grid-pwa-prod heat-pwa-prod pie-pwa-prod",
    "install-bar-pwa": "cd bar-pwa && npm install",
    "install-bubble-pwa": "cd bubble-pwa && npm install",
    "install-graph-pwa": "cd graph-pwa && npm install",
    "install-grid-pwa": "cd grid-pwa && npm install",
    "install-heat-pwa": "cd heat-pwa && npm install",
    "install-pie-pwa": "cd pie-pwa && npm install",
    "install-server": "cd server && npm install",
    "install-all": "npm-run-all --parallel --aggregate-output install-server install-bar-pwa install-bubble-pwa install-graph-pwa install-grid-pwa install-heat-pwa install-pie-pwa"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.3"
  }
}
