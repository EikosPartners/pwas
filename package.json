{
  "name": "pwas",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "bar-pwa": "cd bar-pwa && npm run serve -- --port 9090",
    "bubble-pwa": "cd bubble-pwa && npm run serve -- --port 9091",
    "line-pwa": "cd line-pwa && npm run serve -- --port 9092",
    "grid-pwa": "cd grid-pwa && npm run serve -- --port 9093",
    "heat-pwa": "cd heat-pwa && npm run serve -- --port 9094",
    "pie-pwa": "cd pie-pwa && npm run serve -- --port 9095",
    "log-pwa": "cd log-pwa && npm run serve -- --port 9096",
    "user-settings-pwa": "cd user-settings-pwa && npm run serve -- --port 9097",
    "metrics-pwa": "cd metrics-pwa && npm run serve -- --port 9098",
    "server": "cd server && nodemon app.js",
    "serve-all": "npm-run-all --parallel --aggregate-output bubble-pwa grid-pwa bar-pwa line-pwa heat-pwa pie-pwa log-pwa user-settings-pwa metrics-pwa",
    "serve-all-no-m": "npm-run-all --parallel --aggregate-output bubble-pwa grid-pwa bar-pwa line-pwa heat-pwa pie-pwa log-pwa user-settings-pwa",
    "build-bar-pwa": "cd bar-pwa && npm run build",
    "build-bubble-pwa": "cd bubble-pwa && npm run build",
    "build-line-pwa": "cd line-pwa && npm run build",
    "build-grid-pwa": "cd grid-pwa && npm run build",
    "build-heat-pwa": "cd heat-pwa && npm run build",
    "build-pie-pwa": "cd pie-pwa && npm run build",
    "build-log-pwa": "cd log-pwa && npm run build",
    "build-user-settings-pwa": "cd user-settings-pwa && npm run build",
    "build-metrics-pwa": "cd metrics-pwa && npm run build",
    "build-all": "npm-run-all --parallel --aggregate-output build-bar-pwa build-bubble-pwa build-line-pwa build-grid-pwa build-heat-pwa build-pie-pwa build-log-pwa build-user-settings-pwa build-metrics-pwa",
    "bar-pwa-built": "cd bar-pwa && http-server dist/ -p 9090",
    "bubble-pwa-built": "cd bubble-pwa && http-server dist/ -p 9091",
    "line-pwa-built": "cd line-pwa && http-server dist/ -p 9092",
    "grid-pwa-built": "cd grid-pwa && http-server dist/ -p 9093",
    "heat-pwa-built": "cd heat-pwa && http-server dist/ -p 9094",
    "pie-pwa-built": "cd pie-pwa && http-server dist/ -p 9095",
    "log-pwa-built": "cd log-pwa && http-server dist/ -p 9096",
    "user-settings-pwa-built": "cd user-settings-pwa && http-server dist/ -p 9097",
    "run-all-built": "npm-run-all --parallel --aggregate-output bar-pwa-built bubble-pwa-built line-pwa-built grid-pwa-built heat-pwa-built pie-pwa-built log-pwa-built user-settings-pwa-built",
    "install-bar-pwa": "cd bar-pwa && npm install",
    "install-bubble-pwa": "cd bubble-pwa && npm install",
    "install-line-pwa": "cd line-pwa && npm install",
    "install-grid-pwa": "cd grid-pwa && npm install",
    "install-heat-pwa": "cd heat-pwa && npm install",
    "install-pie-pwa": "cd pie-pwa && npm install",
    "install-log-pwa": "cd log-pwa && npm install",
    "install-user-settings-pwa": "cd user-settings-pwa && npm install",
    "install-metrics-pwa": "cd metrics-pwa && npm install",
    "install-server": "cd server && npm install",
    "install-all": "npm-run-all --serial install-server install-bar-pwa install-bubble-pwa install-line-pwa install-grid-pwa install-heat-pwa install-pie-pwa install-user-settings-pwa install-log-pwa install-metrics-pwa",
    "update-bar-pwa": "cd bar-pwa && npm update",
    "update-bubble-pwa": "cd bubble-pwa && npm update",
    "update-line-pwa": "cd line-pwa && npm update",
    "update-grid-pwa": "cd grid-pwa && npm update",
    "update-heat-pwa": "cd heat-pwa && npm update",
    "update-pie-pwa": "cd pie-pwa && npm update",
    "update-log-pwa": "cd log-pwa && npm update",
    "update-user-settings-pwa": "cd user-settings-pwa && npm update",
    "update-metrics-pwa": "cd metrics-pwa && npm update",
    "update-server": "cd server && npm update",
    "update-all": "npm-run-all --parallel --aggregate-output update-server update-bar-pwa update-bubble-pwa update-line-pwa update-grid-pwa update-heat-pwa update-pie-pwa update-log-pwa update-user-settings-pwa update-metrics-pwa",
    "test-bar-pwa": "cd bar-pwa && npm run test:unit",
    "test-bubble-pwa": "cd bubble-pwa && npm run test:unit",
    "test-line-pwa": "cd line-pwa && npm run test:unit",
    "test-grid-pwa": "cd grid-pwa && npm run test:unit",
    "test-heat-pwa": "cd heat-pwa && npm run test:unit",
    "test-pie-pwa": "cd pie-pwa && npm run test:unit",
    "test-log-pwa": "cd log-pwa && npm run test:unit",
    "test-user-settings-pwa": "cd user-settings-pwa && npm run test:unit",
    "test-server": "cd server && npm run test:unit",
    "test-all": "npm-run-all test-bar-pwa test-bubble-pwa test-grid-pwa test-heat-pwa test-line-pwa test-log-pwa test-pie-pwa test-user-settings-pwa"
  },
  "devDependencies": {
    "@vue/cli": "^3.0.4",
    "npm-run-all": "^4.1.3"
  },
  "dependencies": {
    "jslinq": "^1.0.21",
    "material-design-icons-iconfont": "^3.0.3",
    "typeface-roboto": "0.0.54",
    "vuetify": "^1.2.4"
  }
}
